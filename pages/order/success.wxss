@import "/style/common.wxss";
.note-box{
    text-align: center;
    margin: 10px 0px;
    margin-bottom: 20px;
}
.note-box .icon-success{
    margin: 10px 0px;
}
.note-box .view-h2{
    font-size: 16px;
    line-height: 32px;
    font-weight: bold;
}
.note-box  .view-p{
    color: #888;
    font-size: 14px;
}
/** button **/
.body .from-group{
    padding: 0px 10px;
    margin: 10px 0px;
}
.body .from-group .next-bnt{
    width: 100%;
    background: #ffad01;
    color: #fff;
}
.body .from-group button{
    width: 100%;
}
.body .bnt{
  width: 100%;
  border: 1px solid #ebebeb;
  border-radius: 5px;
  background: #f8f8f8;
  text-align: center;
  line-height: 42px;
  font-size: 14px;
  margin: 5px 0px;
}